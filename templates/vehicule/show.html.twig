{% extends 'baseback.html.twig' %}

{% block title %}Vehicule{% endblock %}

{% block main %}
    <h1 style="text-align: center; margin-top: 50px;margin-bottom: 50px;">Vehicule</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Matricule</th>
                <td>{{ vehicule.matricule }}</td>
            </tr>
            <tr>
                <th>Type of vehicle</th>
                <td>{{ vehicule.typeVehicule }}</td>
            </tr>
            <tr>
                <th>Brand</th>
                <td>{{ vehicule.marqueVehicule }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_vehicule_index') }}">Back to list</a>
    <br>
    <a href="{{ path('app_vehicule_edit', {'matricule': vehicule.matricule}) }}">Edit</a>

    {{ include('vehicule/_delete_form.html.twig') }}
{% endblock %}
